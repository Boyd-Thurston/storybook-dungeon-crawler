{
  "name": "storybook-dungeon-crawler",
  "version": "0.0.2",
  "description": "A react based browser game",
  "repository": "https://github.com/Boyd-Thurston/storybook-dungeon-crawler.git",
  "main": "server/index.js",
  "author": "Boyd Thurston",
  "license": "ISC",
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "webpack --watch",
    "dev:server": "nodemon server",
    "start": "run-s build server",
    "build": "webpack",
    "server": "node server",
    "test": "jest --watchAll",
    "knex": "knex --knexfile server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "authenticare": "^0.4.6",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.2.0",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
